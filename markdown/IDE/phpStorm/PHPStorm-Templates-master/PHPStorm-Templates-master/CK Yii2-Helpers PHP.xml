  <template name="yii-icon" value="echo Html::icon('$icon$');" description="Yii2-helpers Bootstrap3 icon" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="icon" expression="" defaultValue="&quot;pencil&quot;" alwaysStopAt="true" />
    <context>
